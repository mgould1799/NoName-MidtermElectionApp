<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.csci360.electionapp.view.VotingAreaController">
   <children>
      <Label alignment="CENTER" layoutX="203.0" layoutY="14.0" prefHeight="25.0" prefWidth="193.0" text="Select Your Candidates Below" textAlignment="CENTER" textFill="#f8f3f3" textOverrun="CENTER_WORD_ELLIPSIS" underline="true">
         <font>
            <Font name="Arial" size="12.0" />
         </font></Label>
      <Label layoutX="272.0" layoutY="72.0" text="Senator" textFill="#f8f7f7">
         <font>
            <Font name="Arial" size="12.0" />
         </font></Label>
      <Label layoutX="427.0" layoutY="72.0" stylesheets="@stylesheet.css" text="House Representive" textFill="#f5f5f5">
         <font>
            <Font name="Arial" size="12.0" />
         </font></Label>
      <Label layoutX="100.0" layoutY="70.0" prefHeight="17.0" prefWidth="69.0" text="Governor" textFill="#f5efef" />
      <ComboBox fx:id="mayorBox" layoutX="55.0" layoutY="110.0" onAction="#initialize" prefWidth="150.0" />
      <ComboBox fx:id="treasurerBox" layoutX="225.0" layoutY="110.0" onAction="#initialize" prefWidth="150.0" />
      <ComboBox fx:id="councilBox" layoutX="406.0" layoutY="110.0" onAction="#initialize" prefWidth="150.0" />
      <Button fx:id="submit" layoutX="503.0" layoutY="346.0" mnemonicParsing="false" onAction="#pressSubmit" text="Submit" />
