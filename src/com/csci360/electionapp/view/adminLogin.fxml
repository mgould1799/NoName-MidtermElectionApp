   <children>
      <ButtonBar layoutX="375.0" layoutY="346.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="adminReturnButton" mnemonicParsing="false" onAction="#pressAdminReturn" text="Return" />
        </buttons>
      </ButtonBar>
      <Button fx:id="adminTallyVotes" layoutX="160.0" layoutY="161.0" mnemonicParsing="false" onAction="#pressTallyVotes" prefHeight="50.0" prefWidth="140.0" text="Tally Votes" />
      <Button fx:id="printVotes" layoutX="300.0" layoutY="161.0" mnemonicParsing="false" onAction="#pressPrintVotes" prefHeight="50.0" prefWidth="140.0" text="Print Votes" />
   </children>
</AnchorPane>
