<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: midnightblue;" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.csci360.electionapp.view.VotingAreaController">
   <children>
      <Label alignment="CENTER" layoutX="55.0" layoutY="15.0" prefHeight="42.0" prefWidth="467.0" text="Select Your Candidates Below" textAlignment="CENTER" textFill="WHITE" textOverrun="CENTER_WORD_ELLIPSIS" underline="true">
         <font>
            <Font name="Arial" size="34.0" />
         </font></Label>
      <Label layoutX="265.0" layoutY="76.0" text="Senator" textFill="WHITE">
         <font>
            <Font name="Arial" size="20.0" />
         </font></Label>
      <Label layoutX="404.0" layoutY="78.0" stylesheets="@stylesheet.css" text="House Representive" textFill="WHITE">
         <font>
            <Font name="Arial" size="17.0" />
         </font></Label>
      <Label layoutX="86.0" layoutY="74.0" prefHeight="26.0" prefWidth="89.0" text="Governor" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font></Label>
      <ComboBox fx:id="mayorBox" layoutX="55.0" layoutY="110.0" onAction="#initialize" prefWidth="150.0" promptText="Click">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="treasurerBox" layoutX="225.0" layoutY="110.0" onAction="#initialize" prefWidth="150.0" promptText="Click">
